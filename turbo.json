{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build:app": {
      "dependsOn": ["^build:app"],
      "outputs": ["dist/**"],
      "cache": true
    },
    "build:api": {
      "dependsOn": ["^build:api"],
      "outputs": ["dist/**"],
      "cache": true
    },
    "build:packages": {
      "dependsOn": ["^build:packages"],
      "outputs": ["dist/**"],
      "cache": true
    },
    "dockerize:api": {
      "dependsOn": ["^build:api"],
      "outputs": ["dist/**"],
      "cache": true
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
