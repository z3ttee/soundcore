{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "compilerOptions": {
    "jsx": "react",
    "lib": ["ES6", "DOM", "ES2016", "ES2017"],
    "module": "ESNext",
    "target": "ES5",
    "declaration": true,
    "declarationDir": "./dist",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true
  }
}
